<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="NetworkBox" parent="GtkBox">
    <child>
      <object class="GtkBox" id="network_box">
        <child>
          <object class="GtkLabel" id="icon">
            <property name="label"></property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">6</property>
            <property name="margin-end">12</property>
          </object>
        </child>
        <child>
          <object class="GtkRevealer" id="network_revealer_box">
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkRevealer" id="network_revealer_label">
                    <child>
                      <object class="GtkLabel" id="network_name">
                        <property name="name">network_name</property>
                        <!-- <property name="visible">false</property> -->
                        <property name="label"></property>
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">6</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
