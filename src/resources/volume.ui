<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="VolumeBox" parent="GtkBox">
    <child>
      <object class="GtkBox" id="volume_box">
        <child>
          <object class="GtkLabel" id="vol_label">
            <property name="label">ï€¨</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
          </object>
        </child>
        <child>
          <object class="GtkRevealer" id="level_bar_revealer">
            <child>
              <object class="GtkLevelBar" id="vol_level_bar">
                <property name="width-request">100</property>
                <property name="margin-top">11</property>
                <property name="margin-bottom">11</property>
                <property name="orientation">horizontal</property>
                <property name="max-value">100</property>
                <property name="min-value">0</property>
                <!-- <property name="value">40</property> -->
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
